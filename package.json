{
  "name": "summergamefest",
  "type": "module",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": "^22"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,json5,yaml,yml,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "pnpm --silent run build.client && pnpm --silent run build.server",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/node-server/vite.config.ts",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "lint": "eslint src",
    "prepare": "simple-git-hooks",
    "serve": "node server/entry.node-server",
    "test": "vitest",
    "typecheck": "tsc --noEmit --project tsconfig.json"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.13.2",
    "@builder.io/qwik": "1.13.0",
    "@builder.io/qwik-city": "1.13.0",
    "@iconify-json/simple-icons": "1.2.33",
    "@tsconfig/node22": "22.0.1",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "22.15.3",
    "@unocss/preset-icons": "66.0.0",
    "@unocss/preset-uno": "66.0.0",
    "@unocss/preset-web-fonts": "66.0.0",
    "@unocss/reset": "66.0.0",
    "@unocss/transformer-directives": "66.0.0",
    "@unocss/vite": "66.0.0",
    "date-fns": "4.1.0",
    "eslint": "9.13.0",
    "eslint-define-config": "2.1.0",
    "eslint-plugin-qwik": "1.9.1",
    "lint-staged": "16.1.0",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "simple-git-hooks": "2.13.0",
    "typescript": "5.8.3",
    "vite": "6.3.4"
  },
  "pnpm": {
    "overrides": {
      "is-core-module": "npm:@nolyfill/is-core-module@^1",
      "array-includes": "npm:@nolyfill/array-includes@^1",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@^1",
      "object.assign": "npm:@nolyfill/object.assign@^1",
      "object.values": "npm:@nolyfill/object.values@^1"
    }
  }
}
