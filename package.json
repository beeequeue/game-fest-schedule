{
  "name": "summergamefest",
  "type": "module",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@9.9.0",
  "engines": {
    "node": "^20"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,json5,yaml,yml,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit --project tsconfig.json",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "date-fns": "3.6.0",
    "vue": "3.4.38"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.7.3",
    "@iconify-json/simple-icons": "1.1.115",
    "@tsconfig/node20": "20.1.4",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "20.16.2",
    "@unocss/preset-icons": "0.62.3",
    "@unocss/preset-uno": "0.62.3",
    "@unocss/preset-web-fonts": "0.62.3",
    "@unocss/reset": "0.62.3",
    "@unocss/transformer-directives": "0.62.3",
    "@unocss/vite": "0.62.3",
    "@vitejs/plugin-vue": "5.1.3",
    "dotenv": "16.4.5",
    "eslint": "9.11.1",
    "eslint-define-config": "2.1.0",
    "lint-staged": "15.2.7",
    "prettier": "3.3.3",
    "simple-git-hooks": "2.11.1",
    "typescript": "5.5.4",
    "vite": "5.4.2",
    "vue-tsc": "2.1.4"
  },
  "pnpm": {
    "overrides": {
      "hasown": "npm:@nolyfill/hasown@latest"
    }
  }
}
